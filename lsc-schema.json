{
    "schemas": [
{
        "name": "LSC",
        "connection": {
            "database": "CottontailConnectionProvider",
            "parameters": {
                "Host": "127.0.0.1",
                "port": "1865"
            }
        },
        "fields": [
            {
                "name": "averagecolor",
                "factory": "AverageColor"
            },
            {
                "name": "file",
                "factory": "FileSourceMetadata"
            },
            {
                "name": "exif",
                "factory": "ExifMetadata",
                "parameters": {
                    "minute_id": "STRING",
                    "utc_time": "DATETIME",
                    "local_time": "DATETIME",
                    "latitude": "FLOAT",
                    "longitude": "FLOAT",
                    "altitude": "FLOAT",
                    "semantic_name": "STRING",
                    "time_zone": "STRING",
                    "heart_rate(bpm)": "STRING",
                    "heart_rate_conf": "STRING",
                    "calories": "FLOAT",
                    "distance": "FLOAT",
                    "artist name": "STRING",
                    "song name": "STRING",
                    "album name": "STRING",
                    "sleep_level": "STRING",
                    "awake": "STRING",
                    "minutesToFallAsleep": "FLOAT",
                    "minutesAsleep": "FLOAT",
                    "minutesAwake": "FLOAT",
                    "minutesAfterWakeup": "FLOAT",
                    "timeInBed": "FLOAT",
                    "sleep_efficiency": "STRING",
                    "row_index": "INT",
                    "GpsDirectory_GPSLatitude": "FLOAT",
                    "GpsDirectory_GPSLongitude": "FLOAT",
                    "GpsDirectory_GPSAltitude": "FLOAT"
                }
            },
            {
                "name": "clip",
                "factory": "DenseEmbedding",
                "parameters": {
                    "host": "http://10.34.58.85:5002",
                    "model": "open-clip-vit-b32",
                    "length":"512"
                }
            },
            {
                "name": "dino",
                "factory": "DenseEmbedding",
                "parameters": {
                    "host": "http://10.34.58.85:5000/",
                    "model": "dino-v2-vits14",
                    "length":"384"
                }
            }
        ],
        "resolvers": {
            "disk": {
                "factory": "DiskResolver",
                "parameters": {
                    "location": "D:/LSC24-mini-thumbs"
                }
            }
        },
        "exporters": [
            {
                "name": "thumbnail",
                "factory": "ThumbnailExporter",
                "resolverName": "disk",
                "parameters": {
                    "maxSideResolution": "300",
                    "mimeType": "JPG"
                }
            }
        ],
        "extractionPipelines": []
    }
]
}
